{
  "applications": {
    "gecko": {
      "id": "mathias@smkid.dk",
      "strict_min_version": "52.0"
    }
  },
  "name": "DTU Course Analyzer",
  "description": "Analyzes and scores courses on kurser.dtu.dk/xxxxx",
  "version": "1.3.5",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://kurser.dtu.dk/course/*",
        "https://kurser.dtu.dk/course/*"
      ],
      "js": [
        "js/jquery.js",
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "contentscript.js"
  ],
  "background": {
    "scripts": [
      "js/jquery.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "https://seb-sti1.github.io/dtu-course-analyzer/data.json"
  ],
  "browser_action": {
    "default_icon": "icons/128.png"
  }
}
